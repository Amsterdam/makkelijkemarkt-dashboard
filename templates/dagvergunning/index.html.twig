{% extends "base.html.twig" %}

{% block title %}Dagvergunningen - {% endblock %}
{% block css %}
        {{ encore_entry_link_tags('app') }}
{% endblock %}
{% block javascript %}
        {{ encore_entry_script_tags('app') }}
{% endblock %}

{% block document %}
    <h1>Dagvergunningen</h1>

    <p>Selecteer een markt a.u.b.</p>

    {% for markt in markten %}
        <p><a href="{{ path('app_dagvergunning_dayview', {'marktId': markt.id, 'dag': dag}) }}">{{ markt.naam }} ({{ markt.afkorting }})</a></p>
    {% endfor %}
{% endblock %}