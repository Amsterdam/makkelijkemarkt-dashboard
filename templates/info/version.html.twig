{% extends 'base.html.twig' %}

{% block title %}Info {% endblock %}

{% block css %}
        {{ encore_entry_link_tags('app') }}
{% endblock %}
{% block javascript %}
        {{ encore_entry_script_tags('app') }}
{% endblock %}

{% block document %}
    <div class="jumbotron">
        <h1>Makkelijke Markt</h1>
        <p>Dashboard versie <strong>{{ dashboardVersion }}</strong> / API version <strong>{{ apiVersion.apiVersion }}</strong></p>
        <p>API locatie: <strong>{{ apiUrl }}</strong></p>
    </div>

    <p class="lead text-center">
        Gemaakt door Johan Groenen, Kamiel Naaktgeboren, Maarten de Keizer, Maarten Geraets, Yvonne de Mey
    </p>

    <p class="text-center">
        met de hulp van Assetic, Backbone, Bootstrap, Doctrine, Guzzle, jQuery, Monolog, Nelmio API doc, Swiftmailer, Symfony, Twig
    </p>
{% endblock %}